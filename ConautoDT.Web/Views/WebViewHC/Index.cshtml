<!DOCTYPE html>
@model VET_ANIMAL.WEB.Models.FichasControlViewModel
@{
    ViewData["Title"] = "HISTORIAL";
}
<html lang="en">
<head>
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/alertifyjs@1.13.1/build/css/alertify.css" />
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/alertifyjs@1.13.1/build/css/themes/bootstrap.css" />
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <link rel="stylesheet" href="~/Content/bootstrap.css" />

    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        .encabezado{
            box-shadow: #7F7FD5 0px 0px 12px 0px;
            border: 1px solid #a12727;
            background: #7F7FD5; /* fallback for old browsers */
            background: -webkit-linear-gradient(to left, #91EAE4, #86A8E7, #7F7FD5); /* Chrome 10-25, Safari 5.1-6 */
            background: linear-gradient(to left, #91EAE4, #86A8E7, #7F7FD5); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
        }

        .buttonMasco {
            display: inline-block;
            text-align: center;
            vertical-align: middle;
            margin-top: 25px;
            padding: 12px 24px;
            border: 1px solid #a12727;
            width:100%;
            border-radius: 3px;
            background: #4d71b0;
            background: -webkit-gradient(linear, left top, left bottom, from(#4d71b0), to(#992727));
            background: -moz-linear-gradient(top, #4d71b0, #992727);
            background: linear-gradient(to bottom, #4d71b0, #992727);
            -webkit-box-shadow: #ff5959 0px 0px 12px 0px;
            -moz-box-shadow: #ff5959 0px 0px 12px 0px;
            box-shadow: #ff5959 0px 0px 12px 0px;
            text-shadow: #591717 1px 1px 1px;
            font: normal normal bold 20px arial;
            color: #ffffff;
            text-decoration: none;
        }

            .buttonMasco:hover,
            .buttonMasco:focus {
                background: #5c88d3;
                background: -webkit-gradient(linear, left top, left bottom, from(#5c88d3), to(#b62f2f));
                background: -moz-linear-gradient(top, #5c88d3, #b62f2f);
                background: linear-gradient(to bottom, #5c88d3, #b62f2f);
                color: #ffffff;
                text-decoration: none;
            }

            .buttonMasco:active {
                background: #2e446a;
                background: -webkit-gradient(linear, left top, left bottom, from(#2e446a), to(#982727));
                background: -moz-linear-gradient(top, #2e446a, #982727);
                background: linear-gradient(to bottom, #2e446a, #982727);
            }

            .buttonMasco:before {
                content: "\0000a0";
                display: inline-block;
                height: 24px;
                width: 24px;
                line-height: 24px;
                margin: 0 4px -6px -4px;
                position: relative;
                top: 0px;
                left: 0px;
                background: url("data:image/png;base64,") no-repeat left center transparent;
                background-size: 100% 100%;
            }


        .buttonFichas {
            display: inline-block;
            text-align: center;
            vertical-align: middle;
            margin-top: 30px;
            margin-bottom: 20px;
            padding: 12px 24px;
            width: 100%;
            border: 1px solid #a12727;
            border-radius: 3px;
            background: #b06d4d;
            background: -webkit-gradient(linear, left top, left bottom, from(#b06d4d), to(#261e1e));
            background: -moz-linear-gradient(top, #b06d4d, #261e1e);
            background: linear-gradient(to bottom, #b06d4d, #261e1e);
            -webkit-box-shadow: #ff9e59 0px 0px 12px 0px;
            -moz-box-shadow: #ff9e59 0px 0px 12px 0px;
            box-shadow: #ff9e59 0px 0px 12px 0px;
            text-shadow: #591717 1px 1px 1px;
            font: normal normal bold 20px arial;
            color: #ffffff;
            text-decoration: none;
        }

            .buttonFichas:hover,
            .buttonFichas:focus {
                background: #d3835c;
                background: -webkit-gradient(linear, left top, left bottom, from(#d3835c), to(#2e2424));
                background: -moz-linear-gradient(top, #d3835c, #2e2424);
                background: linear-gradient(to bottom, #d3835c, #2e2424);
                color: #ffffff;
                text-decoration: none;
            }

            .buttonFichas:active {
                background: #6a412e;
                background: -webkit-gradient(linear, left top, left bottom, from(#6a412e), to(#261e1e));
                background: -moz-linear-gradient(top, #6a412e, #261e1e);
                background: linear-gradient(to bottom, #6a412e, #261e1e);
            }

            .buttonFichas:before {
                content: "\0000a0";
                display: inline-block;
                height: 24px;
                width: 24px;
                line-height: 24px;
                margin: 0 4px -6px -4px;
                position: relative;
                top: 0px;
                left: 0px;
                background: url("data:image/png;base64,") no-repeat left center transparent;
                background-size: 100% 100%;
            }
    </style>
</head>


<body>
    <div class="container" style="border: outset 3px">
        <center>
            <h2 class="mb-3" style="margin-top:20px; color: black; font-weight: bold; font-size: 2.2em;font-family: 'Young Serif', sans-serif;">HISTORIA CLINICA </h2>
        </center>

        <div class="container" style="margin-top: 15px;">
            <div class="row justify-content-center">
                <div class="col-md-6">
                    <form style="color: black;font-family: 'Young Serif', sans-serif;">
                        <input type="hidden" class="form-control" id="CI" placeholder="Ingrese Cédula" onkeypress="return solo10Numeros(event)" required readonly>

                        <input type="hidden" class="form-control" id="cedCliente" readonly style="color: red;">

                        <div class="mb-3">
                            <label for="petDropdown" class="form-label">Mascota</label>
                            <select class="form-select" id="petDropdown" readonly style="color: blue;">
                                <!-- Opciones de mascotas -->
                            </select>
                        </div>
                    </form>

                </div>
            </div>
        </div>

        <!-- Botones que ocupan toda la pantalla -->
        <div class="row mt-5" style="display: none;">
            <div class="col-12">
                <button type="button" class="btn btn-primary btn-lg btn-block" onclick="mostrarFormulario('formulario1')">DATOS DEL CLIENTE</button>
            </div>
        </div>

        <div class="row mt-2" id="formularioContainer1" style="display: none;">
            <div class="row col-md-8" style="margin-left: 20%;margin-top: 2%;font-family: 'Young Serif', sans-serif; ">


                <div class="form-group col-md-6">
                    <label for="nombreCliente">Nombres</label>
                    <input type="text" style="color: blue;font-size: 14px;" id="nombreCliente" class="form-control" readonly />
                </div>

                <div class="form-group col-md-6">
                    <label for="dirCliente">Dirección</label>
                    <input type="text" style="color: blue;font-size: 14px;" id="dirCliente" class="form-control" readonly />
                </div>

                <div class="form-group col-md-6">
                    <label for="correoCliente">Correo</label>
                    <input type="text" onkeypress="return validarCorreoElectronico(event)" style="color: blue;font-size: 14px;" id="correoCliente" class="form-control" required readonly />
                </div>

                <div class="form-group col-md-6">
                    <label for="telefono">Teléfono</label>
                    <input type="text" onkeypress="return solo10Numeros(event)" style="color: blue;font-size: 14px;" id="telefono" class="form-control" readonly />
                </div>

            </div>
        </div>
        <center>
            <div class="row">
                <div class="col-12">
                    <button type="button" class="buttonMasco" onclick="mostrarFormulario('formulario2')">DATOS MASCOTA</button>
                </div>
            </div>
        </center>


        <div class="row mt-2" id="formularioContainer2" style="display: none;">


            <div class="form-row mt-2" style=" margin-top: 2%;font-family: 'Young Serif', sans-serif; ">
                
                <div class="form-group col-md-6">
                    <label for="">Nombre</label>
                    <input type="text" style="color: blue;font-size: 16px;" class="form-control" id="editNombre2" readonly>
                </div>
                <br>
                <div class="form-group col-md-6">
                    <label for="">Dueño</label>
                    <input id="editdueño22" type="text" class="form-control mb-3" style="width:100%!important; color: red" readonly>
                </div>
                <br>
                <div class="form-group col-md-6" hidden>
                    <label for="">idmasc</label>
                    <input id="idmasc2" type="text" class="form-control mb-3" style="width:100%!important; color: red" readonly>
                </div>
                <br>
                <div class="form-group col-md-6">
                    <label for="">Peso</label>
                    <input type="text" style="color: blue;font-size: 16px;" class="form-control" id="editPeso2" readonly onkeypress="return soloNumeros(event)" required>
                </div>
                <br>
                <div class="form-group col-md-6">
                    <label for="">Raza</label>
                    <select readonly class="form-control" id="editRaza2" style="color: blue;font-size: 16px;" required>
                        <option value="">Seleccione</option>
                        <option value="Labrador">Labrador</option>
                        <option value="Bulldog">Bulldog</option>
                        <option value="Mestizo">Mestizo</option>
                        <option value="Chihuahua">Chihuahua</option>
                        <option value="French">French</option>
                        <option value="Golden Retriever">Golden Retriever</option>
                        <option value="Pitbull">Pitbull</option>
                    </select>
                </div>

                <br>
                <div class="form-group col-md-6">
                    <label for="">Sexo</label>
                    <select class="form-control" id="editSexo2" style="color: blue;font-size: 16px;" readonly required>
                        <option value="">Seleccione</option>
                        <option value="Macho">Macho</option>
                        <option value="Hembra">Hembra</option>
                    </select>
                </div>
                <br>

                <div class="form-group col-md-6">
                    <label for="">Fecha de Nacimiento</label>
                    <input style="color: blue;font-size: 16px;" class="form-control" id="editFecha2" name="editFecha2" readonly max="2024-12-31" type="text">
                </div>


            </div>

        </div>
        <center>
            <div class="row">
                <div class="col-12">
                    <button type="button" class="buttonFichas" onclick="mostrarFormulario('formulario3')">FICHAS CONSULTAS</button>
                </div>
            </div>
        </center>


        <div class="row mt-2" id="formularioContainer3" style="display: none;font-family: 'Young Serif', sans-serif;">

            <div class="card text-center mt-2">
                <div class="card-header encabezado" style="color: black; font-weight: bold; font-size: 1.2em;font-family: 'Young Serif', sans-serif;">
                    FICHAS CONTROL
                </div>
                <div class="card-body">

                    <div class="table-responsive" style="font-family: 'Young Serif', sans-serif; ">
                        <table class="table table-bordered table-sm tablaMascotas dataTable table-striped" id="tablaMascotas">
                            <thead>
                                <tr>
                                    <th>
                                        Cod.
                                    </th>
                                    <th>
                                        Historia Clinica
                                    </th>
                                    <th>
                                        Motivo Consulta
                                    </th>
                                    <th>
                                        Peso
                                    </th>
                                    <th>
                                        Observacion
                                    </th>
                                    <th>
                                        Fecha
                                    </th>


                                </tr>
                            </thead>
                            <tbody>
                            </tbody>

                        </table>
                    </div>


                </div>
                <div class="card-footer text-muted">
                </div>
            </div>



            <div class="card text-center mt-2">
                <div class="card-header encabezado" style="color: black; font-weight: bold; font-size: 1.2em;font-family: 'Young Serif', sans-serif;">
                    FICHAS HEMOPARASITOSIS
                </div>
                <div class="card-body">
                    <div class="table-responsive" style="font-family: 'Young Serif', sans-serif;">
                        <table class="table table-bordered table-sm  dataTable table-striped" id="tablaFichasHemo">
                            <thead>
                                <tr>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Cod.
                                    </th>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Historia Clinica
                                    </th>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Motivo Consulta
                                    </th>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Diagnostico
                                    </th>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Tratamiento
                                    </th>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Observacion
                                    </th>
                                    <th style="background: #021D5D; color: white;font-size: 14px;font-family: 'Young Serif', sans-serif;">
                                        Fecha
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>

                        </table>
                    </div>
                </div>
                <div class="card-footer text-muted">
                </div>
            </div>


        </div>
    </div>

    <script>
        var tk;
        function mostrarFormulario(formulario) {
            var container = $('#formularioContainer' + formulario.charAt(formulario.length - 1));

            // Toggle la visibilidad del formulario
            container.slideToggle();


        }



        function buscarCliente2() {

            console.log("Iniciando función buscarCliente2");

            // Obtener el valor del input de cédula
            let params = new URLSearchParams(location.search);
            var contract = params.get('cedula');
            tk = params.get('token');
            $("#CI").val(contract);
            var identificacion = $("#CI").val();
            console.log("Cédula veri:", contract);
            console.log("Token:", tk);

            // Hacer la solicitud al endpoint de tu API para consultar el cliente
            $.ajax({
                url: '/HistoriaClinica/BuscarClientePorCIMobile',
                method: 'GET',
                data: { CI: identificacion, token: tk },
                contentType: 'application/json',
                success: function (data) {
                    console.log("Datos del cliente:", data);

                    // Verifica si se está ejecutando y muestra los datos en la consola


                    // Asegúrate de que los identificadores coincidan con los elementos HTML reales
                    $("#nombreCliente").val(data.nombres);
                    $("#dirCliente").val(data.direccion);
                    $("#correoCliente").val(data.correo);
                    $("#telefono").val(data.telefono);
                    $("#cedCliente").val(data.nombres);
                    $("#editdueño22").val(data.nombres);// Cambiado a data.cedula si es el nombre del cliente

                },
                error: function (error) {
                    // Imprime el mensaje de error en la consola para facilitar la depuración
                    console.error('Error en la solicitud AJAX:', error);
                }
            });

            console.log("Finalizando función buscarCliente2");

            var identificacion = $("#CI").val();
            $.ajax({
                url: '/HistoriaClinica/BuscarMascotaPorCIMobile',
                method: 'GET',
                data: { CI: identificacion, token: tk },
                contentType: 'application/json',
                success: function (data) {

                    var dropdown = $("#petDropdown");
                    dropdown.empty(); // Clear existing options

                    // Agrega la opción por defecto "Seleccione"
                    dropdown.append($('<option></option>').val('').html('Seleccione'));

                    // Populate the dropdown with pet names
                    data.forEach(function (pet) {
                        dropdown.append($('<option></option>').val(pet.idMascota).html(pet.nombreMascota));
                    });


                    // Verifica si se está ejecutando y muestra los datos en la consola
                    console.log("Datos MASCOTAAAA:", data);
                    $("#petDropdown").off().change(function () {
                        var selectedPetId = $(this).val();

                        // Find the selected pet in the data array
                        var selectedPet = data.find(function (pet) {
                            return pet.idMascota == selectedPetId;
                        });
                        var longId = parseInt(selectedPet.idMascota); // Assuming it's an integer
                        // Fill the form with the selected pet's details
                        $("#editNombre2").val(selectedPet.nombreMascota);
                        $("#editPeso2").val(selectedPet.peso);
                        $("#editRaza2").val(selectedPet.raza);
                        $("#editSexo2").val(selectedPet.sexo);
                        $("#idmasc2").val(longId);
                        // Verificar si la fecha de nacimiento es válida
                        if (selectedPet.fechaNacimiento) {
                            const fechaNacimiento = new Date(selectedPet.fechaNacimiento);

                            // Verificar si la fecha es válida
                            if (!isNaN(fechaNacimiento.getTime())) {
                                // Formatear la fecha a 'YYYY-MM-DD' para asignarla al input
                                const fechaFormateada = fechaNacimiento.toISOString().split('T')[0];
                                $("#editFecha2").val(fechaFormateada);
                                console.log("Fecha de nacimiento en el modal:", fechaFormateada);
                            } else {
                                console.error('Fecha de nacimiento no válida:', selectedPet.fechaNacimiento);
                            }
                        } else {
                            console.error('La mascota seleccionada no tiene fecha de nacimiento.');
                        }

                        $.ajax({
                            url: '/HistoriaClinica/BuscarFichasControlByIdMobile',
                            method: 'GET',
                            data: { CI: longId, token: tk },
                            contentType: 'application/json',
                            success: function (data) {
                                console.log("aquiii historial", data)

                                // Obtén la referencia del cuerpo de la tabla
                                var tableBody = $("#tablaMascotas tbody");

                                // Limpiar filas existentes
                                tableBody.empty();

                                if (data && data.fichasControl && data.fichasControl.length > 0) {
                                    // Iterar sobre las fichas de control y agregar filas al cuerpo de la tabla
                                    data.fichasControl.forEach(function (fichaControl) {
                                        var row = $("<tr>");
                                        row.append($("<td>").text(fichaControl.codigoFichaControl));
                                        row.append($("<td>").text(data.idHistoriaClinica));
                                        row.append($("<td>").text(fichaControl.motivo));
                                        row.append($("<td>").text(fichaControl.peso));
                                        row.append($("<td>").text(fichaControl.observacion));
                                        // Manipula la cadena de fecha para mostrar solo la fecha
                                        var fechaPartes = fichaControl.fecha.split('T');
                                        var fechaSolo = fechaPartes[0];
                                        row.append($("<td>").text(fechaSolo));
                                        tableBody.append(row);
                                    });
                                } else {
                                    console.log("No hay fichas de control para mostrar.");
                                }


                            },
                            error: function (error) {
                                // Imprime el mensaje de error en la consola para facilitar la depuración
                                console.error('Error en la solicitud AJAX:', error);
                            }
                        });

                        $.ajax({
                            url: '/HistoriaClinica/BuscarFichasHemoByIdMobile',
                            method: 'GET',
                            data: { CI: longId, token: tk },
                            contentType: 'application/json',
                            success: function (content) {
                                console.log("aquiii Hemoooooooo", content)

                                // Obtén la referencia del cuerpo de la tabla
                                var tableBody = $("#tablaFichasHemo tbody");

                                // Limpiar filas existentes
                                tableBody.empty();

                                if (content && content.fichasHemoparasitosis && content.fichasHemoparasitosis.length > 0) {
                                    // Iterar sobre las fichas de control y agregar filas al cuerpo de la tabla
                                    content.fichasHemoparasitosis.forEach(function (fichasHemoparasitosis) {
                                        var row = $("<tr>");
                                        row.append($("<td>").text(fichasHemoparasitosis.codigoFichaHemo).css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));
                                        row.append($("<td>").text(content.idHistoriaClinica).css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));
                                        row.append($("<td>").text("EMERGENCIA").css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));
                                        row.append($("<td>").text(fichasHemoparasitosis.enfermedad).css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));
                                        row.append($("<td>").text(fichasHemoparasitosis.tratamiento).css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));
                                        row.append($("<td>").text(fichasHemoparasitosis.observaciones).css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));

                                        // Formatea la fecha
                                        var fecha = new Date(fichasHemoparasitosis.fecha);
                                        var fechaFormateada = fecha.toLocaleDateString(); // Puedes ajustar el formato según tus preferencias

                                        row.append($("<td>").text(fechaFormateada).css({ "font-size": "14px", "font-family": "'Young Serif', sans-serif" }));

                                        // Agrega la fila al cuerpo de la tabla
                                        tableBody.append(row);
                                    });
                                } else {
                                    console.log("No hay fichas de control para mostrar.");
                                }


                            },
                            error: function (error) {
                                // Imprime el mensaje de error en la consola para facilitar la depuración
                                console.error('Error en la solicitud AJAX:', error);
                            }
                        });

                        console.log("Finalizando función buscarCliente2");

                    });



                },
                error: function (error) {
                    // Imprime el mensaje de error en la consola para facilitar la depuración
                    console.error('Error en la solicitud AJAX:', error);
                }
            });

            console.log("Finalizando función buscarCliente2");

            var idMascota = $("#idmasc2").val();


        }


        buscarCliente2();

    </script>

</body>
</html>