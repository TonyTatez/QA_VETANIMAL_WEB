    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/echarts@5.4.3/dist/echarts.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag@2.0.1/dist/js/multi-select-tag.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            ObtenerResultadosComparativos();
        });
        function ObtenerResultadosComparativos() {
            $.ajax({
                url: '/Testing/ResultadosComparativos',
                method: 'GET',
                data: {},
                contentType: 'application/json',
                success: function (content) {
                    console.log("aquiii Comparacion", content)
                    // Calcula el porcentaje de efectividad
                    var efectividad = (content.resultadoAcertado / content.totalRegistros) * 100;

                    // Actualiza el contenido del div con el id "puntaje"
                    $('#puntaje').text(efectividad.toFixed(2) + '%');
                    $('#totalfrotis').text(content.totalRegistros);
                    $('#aciertofrotis').text(content.resultadoAcertado);
                    $('#errorfrotis').text(content.resultadoErroneo);


                },
                error: function (error) {
                    // Imprime el mensaje de error en la consola para facilitar la depuración
                    console.error('Error en la solicitud AJAX:', error);
                }
            });
        }

        $(document).ready(function () {
            ejecutarFuncionInicial();
        });

        function ejecutarFuncionInicial() {

            $.ajax({
                url: '/Testing/Examenes',
                method: 'GET',

                contentType: 'application/json',
                success: function (data) {
                    console.log('Respuesta exitosa:', data);

                    // Asignar los valores de diagnóstico algoritmo y frotis a los elementos HTML
                    $("#algo").text(data.diagnosticoAlgoritmo);
                    $("#frotis").text(data.diagnosticoFrotis);

                    var myChart = echarts.init(document.getElementById('supportRequestChart'));

                    // Datos para el gráfico de pastel
                    var pieData = [
                        { name: 'Diagnóstico Algoritmo', value: data.diagnosticoAlgoritmo },
                        { name: 'Diagnóstico Frotis', value: data.diagnosticoFrotis }
                    ];

                    // Configuración del gráfico
                    var option = {
                        tooltip: {
                            trigger: 'item',
                            formatter: '{a} <br/>{b}: {c} '
                        },
                        legend: {
                            orient: 'vertical',
                            left: 10,
                            data: ['Diagnóstico Algoritmo', 'Diagnóstico Frotis']
                        },
                        series: [
                            {
                                name: 'Diagnóstico',
                                type: 'pie',
                                radius: ['60%', '40%'],
                                avoidLabelOverlap: false,
                                label: {
                                    show: false,
                                    position: 'center'
                                },
                                emphasis: {
                                    label: {
                                        show: true,
                                        fontSize: '30',
                                        fontWeight: 'bold'
                                    }
                                },
                                labelLine: {
                                    show: false
                                },
                                data: pieData
                            }
                        ]
                    };

                    // Aplicar la opción al gráfico
                    myChart.setOption(option);


                },
                error: function (xhr, textStatus, errorThrown) {
                    // Manejar errores de la solicitud
                    console.error('Error en la solicitud:', errorThrown);
                }
            });
        }









