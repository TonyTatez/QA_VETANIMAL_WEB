        function editarMascota(Mascota) {
            console.log("esditarrr", Mascota);
            $('#editNombre').val(Mascota.nombreMascotas);
            $('#editIdCliente2').val(Mascota.idCliente);
            $('#editdueño2').val(Mascota.dueño);
            $('#editPeso').val(Mascota.peso);
            $('#editRaza').val(Mascota.raza);
            $('#idMascota2').val(Mascota.idMascota);
            $('#editSexo').val(Mascota.sexo);
            $('#hcficha').val(Mascota.idHistoriaClinica);
            // Obtener la fecha de nacimiento y formatearla
            var fechaNacimiento = "";
            if (Mascota.fechaNacimiento) {
                // Dividir la cadena de fecha en partes (dd/mm/aaaa hh:mm:ss)
                var partesFecha = Mascota.fechaNacimiento.split(" ");
                var partesFechaNum = partesFecha[0].split("/");

                // Crear un objeto Date con las partes de la fecha
                var fecha = new Date(partesFechaNum[2], partesFechaNum[1] - 1, partesFechaNum[0]);

                // Formatear la fecha según el formato deseado (yyyy-MM-dd)
                fechaNacimiento = fecha.getFullYear() + "-" +
                    ("0" + (fecha.getMonth() + 1)).slice(-2) + "-" +
                    ("0" + fecha.getDate()).slice(-2);
            }

            $("#editFecha2").val(fechaNacimiento);
        }

